{
  "name": "webPageListMaker",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "気になったページをリスト化!!!",
  "background": {
    "scripts": ["post.js", "jquery-2.1.4.min.js"],
    "persistent": false
  },
  "permissions": [
  	"http://localhost:8888/",
    "tabs",
    "clipboardWrite"
  ],
  "commands": {
    "add": {
      "suggested_key": {
        "default": "Ctrl+Shift+1",
        "mac": "MacCtrl+Shift+1"
      },
      "description": "追加"
    },
    "myCommand2": {
      "suggested_key": {
        "default": "Ctrl+Shift+2",
        "mac": "MacCtrl+Shift+2"
      },
      "description": "取得"
    }
  }
}